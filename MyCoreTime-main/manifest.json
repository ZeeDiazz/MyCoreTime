{
  "manifest_version": 3,
  "name": "MyTimeLogger",
  "description": "A timer extension that logs time to a text file",
  "version": "0.1.0",
  "icons" : {
    "16": "/images/Tiger16.png",
    "48": "/images/Tiger48.png",
    "128": "/images/Tiger128.png"
  },
  "page_action": {
    "default_icon": {
    "16": "/images/Tiger16.png",
    "48": "/images/Tiger48.png",
    "128": "/images/Tiger128.png"
    }
  },
  "author": "Zaid Cossai",
  "background": {
    "service_worker": "background.js"
  },
  "action":{
    "default_popup": "index.html",
    "default_title": "MyTimeLogger"
  }
}